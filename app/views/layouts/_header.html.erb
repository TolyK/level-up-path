<header>
  <nav id="navbar">
    <ul>
      <li id="title">
        <%= link_to root_path do %>
          LevelUp<span class="text-primary">Path</span>
        <% end %>
      </li>
      <% if user_signed_in? %>
        <li><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
